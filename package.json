{
  "name": "yatra-asi",
  "description": "Yatra ASI Project",
  "author": "yatra online pvt ltd",
  "private": true,
  "scripts": {
    "dev": "node server",
    "prod": "cross-env NODE_ENV=production && npm run forev",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf yatra_asi && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "forev": "cross-env NODE_ENV=production forever  start server.js && forever list",
    "list": "forever list",
    "stop": "forever stopall"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.0",
    "cross-env": "^5.0.5",
    "es6-promise": "^4.1.1",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "forever": "^0.15.3",
    "lru-cache": "^4.1.1",
    "morgan": "^1.9.0",
    "request": "^2.83.0",
    "route-cache": "0.4.3",
    "vue": "^2.4.4",
    "vue-resource": "^1.3.4",
    "vue-router": "^2.7.0",
    "vue-scrollto": "^2.8.0",
    "vue-server-renderer": "^2.4.4",
    "vuejs-datepicker": "^0.9.20",
    "vuetify": "0.17.0",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^4.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "chokidar": "^1.7.0",
    "compression-webpack-plugin": "^1.0.1",
    "css-loader": "^0.28.7",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "node-sass": "^4.6.1",
    "opn": "^5.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.0.0",
    "webpack-node-externals": "^1.6.0"
  }
}
